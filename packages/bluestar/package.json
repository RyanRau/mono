{
  "name": "bluestar",
  "version": "1.0.0",
  "description": "(Ozark) Bluestar a react component library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "prepare": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "license": "ISC",
  "peerDependencies": {
    "goober": "^2.1.18",
    "react": "^18 || ^19"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "8.6.16",
    "@storybook/react": "8.6.16",
    "@storybook/react-vite": "8.6.16",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "goober": "^2.1.18",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "storybook": "8.6.16",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^6.0.0"
  }
}
